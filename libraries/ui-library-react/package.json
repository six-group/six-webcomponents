{
  "name": "@six-group/ui-library-react",
  "description": "React wrapper components for @six-group/ui-library",
  "version": "5.0.0",
  "type": "module",
  "private": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./next": {
      "types": "./dist/components.server.d.ts",
      "import": "./dist/components.server.js"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "build": "npm run tsc",
    "tsc": "tsc -p . --outDir ./dist",
    "watch": "npm run tsc -- --watch"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/react": "^19",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "typescript": "^5"
  },
  "dependencies": {
    "@stencil/react-output-target": "^1.2.0"
  },
  "peerDependencies": {
    "@six-group/ui-library": "*"
  }
}

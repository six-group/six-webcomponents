{
  "name": "@six-group/ui-library-react",
  "version": "4.2.4",
  "description": "React wrapper components for @six-group/ui-library",
  "license": "Apache-2.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "build": "npm run clean && npm run compile",
    "clean": "rimraf dist",
    "compile": "tsc && vite build",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^28.0.0",
    "@types/react": "^18.2.74",
    "@types/react-dom": "^18.2.24",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@wdio/browser-runner": "^8.35.1",
    "@wdio/cli": "^8.35.1",
    "@wdio/globals": "^8.36.0",
    "@wdio/mocha-framework": "^8.35.0",
    "@wdio/spec-reporter": "^8.32.4",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "jest": "^28.0.0",
    "react": "^18.2.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "vite": "^5.2.0",
    "wdio-vite-service": "^1.0.6"
  },
  "dependencies": {
    "@stencil/react-output-target": "^0.7.0",
    "@popperjs/core": "^2.9.2",
    "react-dom": "^18.2.0",
    "@six-group/ui-library": "*"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "jest": {
    "preset": "ts-jest",
    "setupTestFrameworkScriptFile": "<rootDir>/jest.setup.js",
    "testPathIgnorePatterns": [
      "node_modules",
      "dist"
    ]
  },
  "engines": {
    "node": ">=16"
  }
}

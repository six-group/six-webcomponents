{
  "name": "@six-group/ui-library-react",
  "description": "React wrapper components for @six-group/ui-library",
  "version": "1.0.0",
  "private": false,
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],

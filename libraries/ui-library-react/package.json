{
  "name": "@six-group/ui-library-react",
  "description": "React wrapper components for @six-group/ui-library",
  "version": "5.0.0-rc.1",
  "type": "module",
  "private": false,
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.js"
    },
    "./next": {
      "types": "./dist/components.server.d.ts",
      "import": "./dist/components.server.js"
    }
  },
  "sideEffects": false,
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "dev": "tsc -p tsconfig.json --watch",
    "build": "npm run tsc"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@stencil/react-output-target": "^1.2.0",
    "@six-group/ui-library": "*",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/node": "^22.15.32",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "typescript": "^5.8.3",
    "vite": "^6.3.5"
  },
  "peerDependencies": {
    "@six-group/ui-library": "*"
  }
}
